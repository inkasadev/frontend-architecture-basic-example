{
	"name": "frontend-architecture-basic-example",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"setup": "npm install && npm run dev",
		"test": "cross-env CI=true react-scripts test",
		"test:dev": "react-scripts test --silent=false",
		"test:coverage": "cross-env CI=true react-scripts test --coverage",
		"sb:dev": "start-storybook -p 6006 -s public",
		"sb:build": "build-storybook -s public",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|json)\"",
		"format": "npm run prettier -- --write",
		"check-format": "npm run prettier -- --list-different",
		"lint": "eslint src/**",
		"validate": "npm run format && npm run lint && npm run test"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"src/App.js",
			"src/index.js",
			"src/Routes.js"
		]
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/user-event": "^13.5.0",
		"classnames": "^2.3.2",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.8.1",
		"react-scripts": "5.0.1",
		"sort-by": "^1.2.0",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack5": "^6.5.16",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/node-logger": "^6.5.16",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"@testing-library/react": "^13.4.0",
		"babel-plugin-named-exports-order": "^0.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"git-commit-msg-linter": "^4.9.4",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"jest-axe": "^7.0.0",
		"jest-environment-jsdom": "^29.4.3",
		"prop-types": "^15.8.1",
		"storybook-addon-react-router-v6": "^0.2.1",
		"webpack": "^5.75.0"
	}
}
